{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; img-src 'self' https://easy-project.vercel.app/favicon.ico; script-src 'self'; style-src 'self';"
      }
    }
  ]
}
